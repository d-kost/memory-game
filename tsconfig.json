{
  "compilerOptions": {
    "target": "es5",
    "jsx": "react",
    "module": "es6",
    "rootDir": "./",
    "moduleResolution": "node",
    "allowJs": true,
    "checkJs": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  }
}
